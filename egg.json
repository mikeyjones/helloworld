{
    "name" : "hello world from the egg",
    "description" : "This is hello world",
    "cmds" : [
                {
                    "cmd" : "controller",
                    "args" : "<name> [hello]",
                    "description" : "creates an hello world project",
                    "files" : [
                        {  
                            "in" : "Controllers/{{name}}Controller.cs", 
                            "out" : "Controller"
                        }
                    ],
                    "postExec" : "dotnet --version"
                }
            ]
}